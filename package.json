{
  "name": "unbag-monorepo",
  "description": "unbag monorepo",
  "private": true,
  "type": "module",
  "scripts": {
    "build:unbag": "pnpm --filter 'unbag' build",
    "build": "pnpm build:unbag",
    "build:docs": "pnpm --filter 'unbag-docs' build",
    "dev": "unbag parallel",
    "clear": "rimraf 'packages/*/{lib,node_modules}' && rimraf node_modules",
    "commit": "unbag commit",
    "test": "echo 'test'",
    "release": "pnpm --filter 'unbag' release",
    "ci:publish": "pnpm release && pnpm publish -r",
    "preinstall": "npx only-allow pnpm",
    "postinstall": "husky && pnpm build:unbag"
  },
  "license": "MIT",
  "author": "LiuWenXing1996 lwx_redstone@163.com",
  "devDependencies": {
    "@commitlint/config-pnpm-scopes": "^19.1.0",
    "@commitlint/types": "^19.0.3",
    "@pnpm/find-workspace-dir": "^7.0.1",
    "@pnpm/workspace.find-packages": "^4.0.9",
    "@types/wait-on": "^5.3.4",
    "husky": "^9.0.11",
    "unbag": "workspace:^",
    "wait-on": "^7.2.0"
  },
  "version": "1.1.0"
}