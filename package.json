{
  "name": "unbag-monorepo",
  "description": "unbag monorepo",
  "private": true,
  "type": "module",
  "scripts": {
    "build:build-tools": "pnpm --filter 'build-tools' build",
    "build:unbag": "pnpm --filter 'unbag' build",
    "build:commands": "pnpm unbag ",
    "init-repo": "pnpm build:build-tools && pnpm build:unbag && unbag init -c ./unbag.init.config.ts",
    "dev": "pnpm build:unbag && unbag build -t commands && unbag parallel -m dev",
    "build:docs": "pnpm --filter 'unbag-docs' build",
    "clear": "rimraf 'packages/*/{lib,node_modules}' && rimraf node_modules",
    "commit": "unbag commit -m dev",
    "test": "echo 'test'",
    "release": "pnpm --filter 'unbag' release",
    "ci:publish": "pnpm release && pnpm publish -r",
    "preinstall": "npx only-allow pnpm",
    "postinstall": "husky",
    "dddd": "pnpm unbag  -c ./unbag.build.config.ts"
  },
  "license": "MIT",
  "author": "LiuWenXing1996 lwx_redstone@163.com",
  "devDependencies": {
    "@commitlint/config-pnpm-scopes": "^19.1.0",
    "@commitlint/types": "^19.0.3",
    "@pnpm/find-workspace-dir": "^7.0.1",
    "@pnpm/workspace.find-packages": "^4.0.9",
    "@types/wait-on": "^5.3.4",
    "build-tools": "workspace:^",
    "create-unbag": "workspace:^",
    "execa": "^9.2.0",
    "husky": "^9.0.11",
    "rimraf": "^6.0.1",
    "unbag": "workspace:^",
    "unbag-command-commit": "workspace:^",
    "unbag-command-create": "workspace:^",
    "unbag-command-parallel": "workspace:^",
    "vite": "^5.4.8",
    "vite-plugin-dts": "^4.2.2",
    "vitest": "^2.1.1",
    "wait-on": "^7.2.0"
  }
}