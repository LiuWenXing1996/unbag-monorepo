{
  "name": "unbag-command-commit",
  "version": "0.0.1",
  "description": "基于 unbag 实现的 commit 命令",
  "type": "module",
  "files": [
    "dist"
  ],
  "license": "MIT",
  "author": "LiuWenXing1996 lwx_redstone@163.com",
  "keywords": [
    "unbag command",
    "commit",
    "git commit"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build -w",
    "release": "unbag release",
    "release-dry": "unbag release -d",
    "test": "vitest",
    "test-watch": " vitest -w"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/esm/index.mjs"
      },
      "require": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/cjs/index.cjs"
      }
    }
  },
  "types": "./dist/types/index.d.ts",
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.mjs",
  "peerDependencies": {
    "unbag": "workspace:^"
  },
  "dependencies": {
    "@commitlint/config-conventional": "^19.4.1",
    "@commitlint/cz-commitlint": "^19.2.0",
    "@commitlint/lint": "^19.2.2",
    "@commitlint/load": "^19.2.0",
    "@commitlint/types": "^19.0.3",
    "core-js-pure": "^3.38.1",
    "execa": "^9.2.0",
    "i18next": "^23.15.1",
    "inquirer": "^9.0.0"
  }
}