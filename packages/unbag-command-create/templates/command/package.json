{
  "name": "command",
  "version": "0.0.1",
  "description": "command template",
  "type": "module",
  "files": [
    "dist"
  ],
  "keywords": [
    "unbag command"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build -w",
    "release": "pnpm build && pnpm entry release",
    "release-dry": "pnpm build && pnpm entry release -d",
    "test": "pnpm entry vitest",
    "test-watch": "pnpm entry vitest -w"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/esm/index.mjs"
      },
      "require": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/cjs/index.cjs"
      }
    }
  },
  "types": "./dist/types/index.d.ts",
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.mjs",
  "dependencies": {
    "@commitlint/config-conventional": "^19.4.1",
    "@commitlint/cz-commitlint": "^19.2.0",
    "@commitlint/lint": "^19.2.2",
    "@commitlint/load": "^19.2.0",
    "@commitlint/types": "^19.0.3",
    "@types/conventional-changelog-core": "^4.2.8",
    "@types/conventional-changelog-writer": "^4.0.10",
    "conventional-changelog": "^6.0.0",
    "conventional-changelog-conventionalcommits": "^8.0.0",
    "conventional-commits-parser": "^6.0.0",
    "conventional-recommended-bump": "^10.0.0",
    "execa": "^9.2.0",
    "lodash": "^4.17.21",
    "inquirer": "^9.0.0",
    "ts-essentials": "^10.0.2",
    "unbag": "workspace:^",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/conventional-changelog": "^3.1.5",
    "@types/debounce-promise": "^3.1.9",
    "@types/deep-freeze-strict": "^1.1.2",
    "@types/lodash": "^4.14.202",
    "@types/semver": "^7.5.8",
    "@types/uuid": "^9.0.2",
    "tsx": "^4.7.1"
  }
}