{
  "name": "unbag",
  "version": "0.11.1",
  "description": "一个专门用来开发npm工具的包",
  "type": "module",
  "files": [
    "bin",
    "dist"
  ],
  "license": "MIT",
  "author": "LiuWenXing1996 lwx_redstone@163.com",
  "keywords": [
    "unbundle",
    "bundleless",
    "npm tool"
  ],
  "bin": {
    "unbag": "./bin/bin.mjs"
  },
  "scripts": {
    "build": "vite build",
    "dev": "vite build -w",
    "release": "pnpm build && pnpm entry release",
    "release-dry": "pnpm build && pnpm entry release -d",
    "test": "vitest",
    "test-watch": "vitest -w"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/esm/index.mjs"
      },
      "require": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/cjs/index.cjs"
      }
    }
  },
  "types": "./dist/types/index.d.ts",
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.mjs",
  "dependencies": {
    "@types/yargs": "^17.0.33",
    "bundle-require": "^4.0.2",
    "chalk": "^5.3.0",
    "conventional-commits-parser": "^6.0.0",
    "core-js-pure": "^3.38.1",
    "deep-freeze-strict": "^1.1.1",
    "detect-indent": "^7.0.1",
    "detect-newline": "^4.0.1",
    "execa": "^9.2.0",
    "fs-extra": "^11.2.0",
    "lodash": "^4.17.21",
    "radash": "^12.1.0",
    "ts-essentials": "^10.0.2",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/deep-freeze-strict": "^1.1.2",
    "@types/fs-extra": "^11.0.4",
    "@types/lodash": "^4.14.202"
  }
}